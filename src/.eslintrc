{
    "plugins": ["solid", "json"],
    "extends": ["eslint:recommended", "plugin:solid/recommended"],
    "rules": {
        "comma-dangle": 0,
        "indent": ["error", 4, {"SwitchCase": 1}],
        "quotes": [2, "double", "avoid-escape"],
        "semi": [2, "always"],
        "camelcase": [0],
        "object-curly-spacing": [0]
    },
    "parserOptions": {
        // Required for certain syntax usages
        "ecmaVersion": 2022
    },
}